	<!-- Light animations - curently visible at day -->
    <animation>
        <type>noshadow</type>
        <object-name>rwy_end_light_red</object-name>
    </animation>

    <animation>
        <type>alpha-test</type>
		<object-name>rwy_end_light_red</object-name>
        <alpha-factor>0.01</alpha-factor>
    </animation>
	
