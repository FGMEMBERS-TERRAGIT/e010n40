	<!-- LOD -->	
	<animation>
		<type>range</type>
		<object-name>rwy_end_light_red</object-name>
		<min-m>0</min-m>
		<max-m>15000</max-m>
	</animation>
	
	<animation>
		<type>dist-scale</type>
		<object-name>rwy_end_light_red</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0.025</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>0.1</dep>
			</entry>
			<entry>
				<ind>1500</ind>
				<dep>0.2</dep>
			</entry>
			<entry>
				<ind>6000</ind>
				<dep>0.7</dep>
			</entry>
			<entry>
				<ind>20000</ind>
				<dep>1.3</dep>
			</entry>
		</interpolation>
	</animation>
	
