<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

<path>Patscherkofel_Sessel1.ac</path>

<!-- Winter Ground -->
	<animation>
		<type>textranslate</type>
		<object-name>ground</object-name>
			<condition>
				<equals>
					<property>sim/startup/season</property>
					<value>winter</value>
				</equals>
			</condition>
			<offset>0.50</offset>
			<center>
				<x-m> 0 </x-m>
				<y-m> 0 </y-m>
				<z-m> 0 </z-m>
			</center>
			<axis> 
				<x> 0 </x>
				<y> -1 </y>
				<z>  0 </z>
			</axis>
	</animation>
	
<!-- Windows Lit -->
	<animation> 
		<type>textranslate</type> 
		<object-name>windowslit</object-name>
			<property>/sim/time/sun-angle-rad</property>
			<step> 1.57 </step>
			<factor>0.318471338</factor>
			<center>
				<x-m> 0 </x-m>
				<y-m> 0 </y-m>
				<z-m> 0 </z-m>
			</center>
			<axis>
				<x> 0 </x>
				<y> -1 </y>
				<z>  0 </z>
			</axis>
	</animation>

	<animation>
		<type>material</type>
		<object-name>windowslit</object-name>
			<condition>
				<greater-than>
					<property>/sim/time/sun-angle-rad</property>
					<value>1.57</value>
				</greater-than>
			</condition>
			<emission>
				<red>   1 </red>
				<green> 1 </green>
				<blue>  1 </blue>
			</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>windowslit</object-name>
			<condition>
				<less-than-equals>
					<property>/sim/time/sun-angle-rad</property>
					<value>1.57</value>
				</less-than-equals>
			</condition>
			<emission>
				<red>   0.0 </red>
				<green> 0.0 </green>
				<blue>  0.0 </blue>
			</emission>
	</animation>

  
