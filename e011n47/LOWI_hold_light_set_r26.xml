<PropertyList>

<!-- HOLD light set - Hold light by longfly-->

	
	<!-- Taxiwy HOLD - Left -->
	<model>
		<path>Models/Airport/RGL.ac</path>
		<offsets>
		  <x-m> 0.000 </x-m>
		  <y-m>-14.500 </y-m>
		  <z-m>-0.120 </z-m>
		  <pitch-deg> 0.00</pitch-deg>
		  <heading-deg>250.0 </heading-deg>
		</offsets>
	</model>
	
		<!-- Taxiwy HOLD - Right -->
	<model>
		<path>Models/Airport/RGL.ac</path>
		<offsets>
		  <x-m>-0.000 </x-m>
